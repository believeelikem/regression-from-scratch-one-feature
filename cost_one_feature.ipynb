   "source": [
    "def compute_cost_one_feature(X,y,w,b):\n",
    "    \n",
    "    m = X.shape[0]\n",
    "    cost = 0\n",
    "    \n",
    "    for i in range(m):\n",
    "        f_wb = w * X[i] + b\n",
    "        err = (f_wb - y[i])**2\n",
    "        cost+=err\n",
    "        \n",
    "    total_cost = 1/(2 * m) * cost\n",
    "    \n",
    "    return total_cost"
   ]
  }
